      },
      {
        "current": {"text": "HIGH|MEDIUM|LOW", "value": "HIGH|MEDIUM|LOW"},
        "hide": 0,
        "label": "Severity",
        "name": "severity",
        "options": [],
        "query": "HIGH|MEDIUM|LOW",
        "skipUrlSync": false,
        "type": "textbox"
      }
    ]
  },
  "time": {"from": "now-6h", "to": "now"},
