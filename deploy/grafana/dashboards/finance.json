{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "-- Grafana --",
          "uid": "grafana"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "PROM_DS"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "id": 1,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(odyssey_http_requests_total{route=~$route,company=~$company,branch=~$branch}[5m])) by (route) or sum(rate(odyssey_http_requests_total{route=~$route}[5m])) by (route)",
          "legendFormat": "{{route}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Request Rate",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROM_DS"},
      "fieldConfig": {"defaults": {"unit": "percent"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "100 * (sum(rate(odyssey_http_requests_total{route=~$route,company=~$company,branch=~$branch,code=~\"5..\"}[5m])) or sum(rate(odyssey_http_requests_total{route=~$route,code=~\"5..\"}[5m]))) / (sum(rate(odyssey_http_requests_total{route=~$route,company=~$company,branch=~$branch}[5m])) or sum(rate(odyssey_http_requests_total{route=~$route}[5m])))",
          "legendFormat": "5xx %",
          "refId": "A"
        }
      ],
      "title": "Error Rate (5xx)",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROM_DS"},
      "fieldConfig": {"defaults": {"unit": "ms"}, "overrides": []},
      "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8},
      "id": 3,
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "1000 * histogram_quantile(0.95, sum(rate(odyssey_http_request_duration_seconds_bucket{route=~$route,company=~$company,branch=~$branch}[5m])) by (le, route))",
          "legendFormat": "{{route}}",
          "refId": "A"
        }
      ],
      "title": "Latency p95",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROM_DS"},
      "fieldConfig": {"defaults": {"unit": "percent"}, "overrides": []},
      "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8},
      "id": 4,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "avg(odyssey_finance_cache_hit_ratio{company=~$company,branch=~$branch}) or avg(odyssey_finance_cache_hit_ratio)",
          "legendFormat": "Cache hit",
          "refId": "A"
        }
      ],
      "title": "Cache Hit Ratio",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROM_DS"},
      "fieldConfig": {"defaults": {"unit": "percent"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 17},
      "id": 5,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "100 * sum(rate(odyssey_jobs_total{job=~\"analytics.*\",status=\"success\"}[5m])) / sum(rate(odyssey_jobs_total{job=~\"analytics.*\",status=~\"success|failure\"}[5m]))",
          "legendFormat": "Success %",
          "refId": "A"
        },
        {
          "expr": "100 * sum(rate(odyssey_jobs_total{job=~\"analytics.*\",status=\"failure\"}[5m])) / sum(rate(odyssey_jobs_total{job=~\"analytics.*\",status=~\"success|failure\"}[5m]))",
          "legendFormat": "Failure %",
          "refId": "B"
        }
      ],
      "title": "Job Success vs Failure",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROM_DS"},
      "fieldConfig": {"defaults": {"unit": "short"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 17},
      "id": 6,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(odyssey_finance_anomalies_total{severity=~$severity,company=~$company,branch=~$branch}[15m])) by (severity) or sum(rate(odyssey_finance_anomalies_total{severity=~$severity}[15m])) by (severity)",
          "legendFormat": "{{severity}}",
          "refId": "A"
        }
      ],
      "title": "Anomaly Trend",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["finance"],
  "templating": {
